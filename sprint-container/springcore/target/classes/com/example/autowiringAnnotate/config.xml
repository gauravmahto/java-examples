<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

  <context:annotation-config/>

  <bean class="com.example.autowiringAnnotate.Maruti" name="marutiOne">
    <property name="make" value="800"></property>
  </bean>

  <bean class="com.example.autowiringAnnotate.Maruti" name="marutiTwo">
    <property name="make" value="Baleno"></property>
  </bean>

  <bean class="com.example.autowiringAnnotate.Car" name="carOne"/>

  <bean class="com.example.autowiringAnnotate.Vehicle" name="vehicleOne"/>

</beans>
